---
const { href, unelevated, outlined, customIcon, onPrimary, newTab } =
  Astro.props;

const Tag = href ? 'a' : 'button';
---

<Tag class='w-fit py-2 px-3 mb-0 flex align-middle justify-center bg-slate-800 text-white border-none rounded-xl font-medium cursor-pointer outline-none' href={href}>
  <slot/>
</Tag>